{
    "manifest_version": 3,
    "name": "Spider Food",
    "version": "1.0",
    "description": "Pushes current URL to an API",
    "permissions": ["scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon.png",
      "128": "icon128.png"
    },
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    }
  }